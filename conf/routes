# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

#GET     /playfairy/download         controllers.HomeController.index()
GET		/playfairy/download/test	com.playfairy.controllers.DownloadController.test()
GET		/playfairy/download/hardcoded	com.playfairy.controllers.DownloadController.hardcoded()
GET		/playfairy/download/index	com.playfairy.controllers.DownloadController.index()
GET		/playfairy/download/ipa		com.playfairy.controllers.DownloadController.ipa()
GET		/playfairy/download/plist	com.playfairy.controllers.DownloadController.plist(id: String?="0")

GET		/playfairy/upload/index		com.playfairy.controllers.UploadController.index()
POST	/playfairy/upload/ipa		com.playfairy.controllers.UploadController.ipa()
GET		/playfairy/upload/genplist	com.playfairy.controllers.UploadController.genplist(id: String?="0")